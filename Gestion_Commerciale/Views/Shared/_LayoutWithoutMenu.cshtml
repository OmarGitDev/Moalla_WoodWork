<script>
    var curWin = 0;
    var openWins = [];
    function OPEN_URL_IN_BLANK(url, CloseAfterAMoment) {
        var str = 'addressbar=0,toolbar=0,scrollbars=1,location=0,statusbar=0,menubar=0,resizable=1,screenX=0,screenY=0,top=0,left=0,maximize=1,';
        str = str + 'height=' + screen.height + ', width=' + screen.width;

        if (curWin == undefined)
            curWin = 0;
        if (curWin > 1 && !openWins[curWin - 2].closed) {
            //openWins[curWin - 1].location.replace(url);
            // openWins[curWin - 1].blur();
            // setTimeout(openWins[curWin - 1].focus, 1000);// openWins[curWin - 1].focus();
            if (!openWins[curWin - 1].closed) {
                openWins[curWin - 2].close();
                openWins[curWin - 2] = openWins[curWin - 1];
            }
            openWins[curWin - 1] = window.open(url, '_blank', str);
        }
        else {
            //curWin = 0;
            openWins[curWin++] = window.open(url, '_blank', str);
        }
        setTimeout(function () {
            if (openWins[0].document.readyState == "complete" && CloseAfterAMoment == true) {
                if (openWins[0].document.body.innerHTML == "") {
                    openWins[0].close();
                }
                //else (alert(openWins[0].document.body.innerHTML));
            }
            else {
                //	alert(openWins[0].document.readyState);
            }
        }, 5000);

        //return openWins[0];
    }

</script>